{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@astropub/flow",
	"description": "Use components to control flow in Astro",
	"version": "0.3.6",
	"type": "module",
	"license": "CC0-1.0",
	"exports": {
		".": {
			"default": "./astro-flow.js",
			"types": "./astro-flow.d.ts"
		},
		"./astro-flow": {
			"default": "./astro-flow.js",
			"types": "./astro-flow.d.ts"
		},
		"./Case": {
			"default": "./lib/AstroCase.astro",
			"types": "./lib/AstroCase.d.ts"
		},
		"./For": {
			"default": "./lib/AstroIterate.astro.js",
			"types": "./lib/AstroIterate.astro.d.ts"
		},
		"./Iterate": {
			"default": "./lib/AstroIterate.astro.js",
			"types": "./lib/AstroIterate.astro.d.ts"
		},
		"./Switch": {
			"default": "./lib/AstroSwitch.astro",
			"types": "./lib/AstroSwitch.d.ts"
		},
		"./When": {
			"default": "./lib/AstroWhen.astro",
			"types": "./lib/AstroWhen.d.ts"
		},
		"./HTMLString": {
			"default": "./lib/HTMLString.js",
			"types": "./lib/HTMLString.d.ts"
		},
		"./iterate": {
			"default": "./lib/iterate.js",
			"types": "./lib/iterate.d.ts"
		},
		"./package": "./package.json",
		"./package.json": "./package.json"
	},
	"main": "astro-flow.js",
	"types": "astro-flow.d.ts",
	"unpkg": "astro-flow.js",
	"jsdelivr": "astro-flow.js",
	"sideEffects": false,
	"files": [
		"astro-flow.js",
		"astro-flow.d.ts",
		"lib"
	],
	"keywords": [
		"astro",
		"astro-component",
		"withastro",
		"ui",
		"perf",
		"performance",
		"condition",
		"conditional",
		"conditionals",
		"conditions",
		"component",
		"else",
		"case",
		"control",
		"each",
		"flow",
		"for",
		"if",
		"loop",
		"of",
		"renderer",
		"statement",
		"switch",
		"when"
	],
	"author": "Jonathan Neal <jonathantneal@hotmail.com>",
	"bugs": "https://github.com/astro-community/flow/issues",
	"homepage": "https://github.com/astro-community/flow",
	"repository": "https://github.com/astro-community/flow.git"
}
